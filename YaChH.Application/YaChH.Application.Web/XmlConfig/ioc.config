<?xml version="1.0" encoding="utf-8"?>
<unity>
  <typeAliases>
    <typeAlias alias="IDatabase" type="YaChH.Data.IDatabase,YaChH.Data" />
    <typeAlias alias="EFDatabase" type="YaChH.Data.EF.Database,YaChH.Data.EF" />
    <typeAlias alias="IDbContext" type="YaChH.Data.EF.IDbContext,YaChH.Data.EF" />
    <!--<typeAlias alias="MySql" type="YaChH.Data.EF.MySqlDbContext,YaChH.Data.EF" />-->
    <typeAlias alias="SqlServer" type="YaChH.Data.EF.SqlServerDbContext,YaChH.Data.EF" />
  </typeAliases>
  <containers>
    <container name="DBcontainer">
      <type type="IDatabase" mapTo="EFDatabase" ></type >
      <!-- 默认数据库软件类型：SqlServer,MySql,Oracle,Access,SQLite-->
      <type type="IDbContext" mapTo="SqlServer" ></type >
      <type type="IDbContext" mapTo="SqlServer" name="SqlServer"></type >
      <!--<type type="IDbContext" mapTo="MySql" name="MySql"></type >-->
    </container>
  </containers>
</unity>